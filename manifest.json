{
  "name": "TA Helper",
  "version": "0.1",
  "description": "A chrome extension that helps my job easier at TA Digital.",
  "permissions": ["activeTab"],
  "content_scripts": [
   {
    "matches": ["*://*.hrm.techaspect.com/*"],
     "run_at": "document_idle",
     "js": ["content.js"]
   }
  ],
  "icons": {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/128.png"
  },
  "manifest_version": 2
}
